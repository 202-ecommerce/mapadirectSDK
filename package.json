{
  "name": "mapadirect-sdk",
  "version": "1.0.0",
  "description": "MapaDirect marketplace API",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "doc-build-fr": "stylemark -i doc/fr -o build/doc/fr -c doc/fr/config.yml",
    "doc-build-en": "stylemark -i doc/en -o build/doc/en -c doc/en/config.yml",
    "doc-watch-fr": "stylemark -i doc/fr -o build/doc -c doc/fr/config.yml -w -b",
    "doc-watch-en": "stylemark -i doc/en -o build/doc -c doc/en/config.yml -w -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/202-ecommerce/mapadirectSDK.git"
  },
  "keywords": [
    "MapaDirect",
    "API",
    "marketplace"
  ],
  "author": "202-ecommerce",
  "bugs": {
    "url": "https://github.com/202-ecommerce/mapadirectSDK/issues"
  },
  "homepage": "https://github.com/202-ecommerce/mapadirectSDK#readme",
  "devDependencies": {
    "stylemark": "^3.0.3"
  }
}
